{
    "name": "@mealplanner/monorepo",
    "private": true,
    "packageManager": "pnpm@9.0.0",
    "description": "MealPlanner monorepo with front, agent, backend (pnpm workspaces)",
    "scripts": {
        "dev:front": "pnpm --filter @mealplanner/front dev",
        "dev:agent": "pnpm --filter @mealplanner/agent dev",
        "dev:backend": "pnpm --filter @mealplanner/backend dev",
        "build": "pnpm -r build",
        "preview": "pnpm --filter @mealplanner/front preview"
    }
}
