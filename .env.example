# ====================
# Configuration - MealPlanner
# Copier ce fichier en .env et remplir les valeurs
# ====================

# === PostgreSQL - API Database ===
API_POSTGRES_USER=mealplanner_api
API_POSTGRES_PASSWORD=mealplanner_api
API_POSTGRES_DB=mealplanner_api
API_HOST_PORT=5432

# === PostgreSQL - Agent Database ===
AGENT_POSTGRES_USER=mealplanner_agent
AGENT_POSTGRES_PASSWORD=mealplanner_agent
AGENT_POSTGRES_DB=mealplanner_agent
AGENT_HOST_PORT=5433

# === API Backend ===
API_PORT=3000
API_DB_URL=postgresql://mealplanner_api:mealplanner_api@localhost:5432/mealplanner_api
JWT_SECRET=dev-secret-change-me-in-production

# === Agent ===
AGENT_PORT=3001
AGENT_DB_URL=postgresql://mealplanner_agent:mealplanner_agent@localhost:5433/mealplanner_agent
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENAI_BASE_URL=https://openrouter.ai/api/v1
API_URL=http://localhost:3000

# === Frontend (Vite) ===
# URLs accessibles depuis le navigateur (adapter selon votre domaine/IP)
VITE_API_URL=http://localhost:3000
VITE_AGENT_URL=http://localhost:3001
